{
  "name": "@simple.mail/mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start --port $(turbo get-mfe-port)",
    "build": "pnpm build:web && echo 'In the future, we should run `eas build` here'",
    "build:web": "expo export --platform web",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web --port 8082",
    "lint": "expo lint",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@simple.mail/eslint-config": "workspace:*",
    "@simple.mail/typescript-config": "workspace:*",
    "@simple.mail/vitest-config": "workspace:*",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~19.1.0",
    "eslint": "^10.0.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-plugin-testing-library": "^7.15.4",
    "react-test-renderer": "19.1.0",
    "typescript": "catalog:"
  },
  "private": true
}
